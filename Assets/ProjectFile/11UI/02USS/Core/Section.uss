.Section-Top {
    cursor: initial;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    width: 100%;
    height: 100px;
    background-color: rgba(187, 187, 187, 255);
}

.Section-Main {
    width: 100%;
    height: 100%;
    padding-left: 16px;
    padding-right: 16px;
    padding-top: 16px;
    padding-bottom: 16px;
    -unity-background-scale-mode: stretch-to-fill;
    background-color: rgba(221, 221, 221, 255);
    justify-content: space-between;
}
